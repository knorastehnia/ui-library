{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "noFallthroughCasesInSwitch": false,
    "noUncheckedSideEffectImports": true,
    "module": "ESNext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
  }
}
